[32m+[m[32m#!/bin/bash[m
[32m+[m[32m# Copyright (C) 2020-2023 Cicak Bin Kadal[m
[32m+[m
[32m+[m[32m# This free document is distributed in the hope that it will be[m[41m [m
[32m+[m[32m# useful, but WITHOUT ANY WARRANTY; without even the implied[m[41m [m
[32m+[m[32m# warranty of MERCHANTABILITY or FITNESS FOR A PARTICULAR PURPOSE.[m
[32m+[m
[32m+[m[32m# REV31: Sun 19 Feb 2023 00:00[m
[32m+[m[32m# REV29: Mon 13 Feb 2023 13:30[m
[32m+[m[32m# REV19: Sun 05 Feb 2023 20:00[m
[32m+[m[32m# REV11: Sun 08 May 2022 06:00[m
[32m+[m[32m# REV02: Sun 19 Sep 2021 15:00[m
[32m+[m[32m# START: Mon 28 Sep 2020 21:00[m
[32m+[m
[32m+[m[32m# ATTN:[m
[32m+[m[32m# You new to set "REC2" with your own Public-Key Identity![m
[32m+[m[32m# Check it out with "gpg --list-key"[m
[32m+[m
[32m+[m[32m# ATTN:[m
[32m+[m[32m# You new to set "REC2" with your own Public-Key Identity![m
[32m+[m[32m# Check it out with "gpg --list-key"[m
[32m+[m[32m# ####################### Replace REC2 ####[m
[32m+[m[32mREC2="F3D519A078B59E1E"[m
[32m+[m[32m# ####################### ####### #### ####[m
[32m+[m[32m# REC1: public key[m
[32m+[m[32mREC1="63FB12B215403B20"[m
[32m+[m[32m# WEEKURL="http://localhost:4000/WEEK/WEEK.txt"[m
[32m+[m[32mWEEKURL="https://os.vlsm.org/WEEK/WEEK.txt"[m
[32m+[m[32mFILES="my*.asc my*.txt my*.sh"[m
[32m+[m[32mSHA="SHA256SUM"[m
[32m+[m[32mRESDIR="$HOME/RESULT/"[m
[32m+[m[32musage()  { echo "Usage: $0 [-w <WEEK>]" 1>&2; exit 1; }[m
[32m+[m[32mnolink() { echo "No LINK $1"            1>&2; exit 1; }[m
[32m+[m
[32m+[m[32m# Check current WEEK[m
[32m+[m[32munset WEEK DEFAULT[m
[32m+[m[32mif [ ! -z "${1##*[!0-9]*}" ] ; then[m
[32m+[m[32m  WEEK=$1[m
[32m+[m[32melif [ -z $1 ] ; then[m
[32m+[m[32m  DEFAULT=1[m
[32m+[m[32melse while getopts ":w:W:" varTMP[m
[32m+[m[32m  do[m
[32m+[m[32m    case "${varTMP}" in[m
[32m+[m[32m     w|W)[m
[32m+[m[32m       WEEK=${OPTARG}[m
[32m+[m[32m       [ ! -z "${WEEK##*[!0-9]*}" ] || usage ;;[m
[32m+[m[32m    esac[m
[32m+[m[32m  done[m
[32m+[m[32m  [ -z $WEEK ] && usage[m
[32m+[m[32mfi[m
[32m+[m
[32m+[m[32mif [ $DEFAULT ] ; then[m
[32m+[m[32m  [[ $(wget $WEEKURL -O- 2>/dev/null) ]] || nolink $WEEKURL[m
[32m+[m[32m  intARR=($(wget -q -O - $WEEKURL | awk '/\| Week / {[m[41m [m
[32m+[m[32m    cmd = "date -d " $2 " +%s"[m
[32m+[m[32m    cmd | getline mydate[m
[32m+[m[32m    close(cmd)[m
[32m+[m[32m    print mydate + (86400 * 6)[m
[32m+[m[32m  }'))[m
[32m+[m[32m  DATE=$( LANG=en_us_8859_1;date -d $(date +%d-%b-%Y) +%s)[m
[32m+[m[32m  for II in ${!intARR[@]} ; do[m
[32m+[m[32m    (( $DATE > ${intARR[$II]} )) || break;[m
[32m+[m[32m  done[m
[32m+[m[32m  WEEK=$II[m
[32m+[m[32m  # echo "DEBUG:TMP:$DEFAULT:W[$WEEK]:$1:$DATE:"[m
[32m+[m[32mfi[m
[32m+[m
[32m+[m[32m(( WEEK > 11 )) && WEEK=11[m
[32m+[m[32mWEEK=$(printf "W%2.2d\n" $WEEK)[m
[32m+[m
[32m+[m[32m# echo $WEEK ; exit[m
[32m+[m
[32m+[m[32m# Is this the correct WEEK?[m
[32m+[m[32mread -r -p "Is this WEEK $WEEK ? [y/N] " response[m
[32m+[m[32mcase "$response" in[m
[32m+[m[32m    [yY][eE][sS]|[yY])[m[41m [m
[32m+[m[32m        ;;[m
[32m+[m[32m    *)[m
[32m+[m[32m        echo "It is not Week $WEEK!"[m
[32m+[m[32m        usage[m
[32m+[m[32m        ;;[m
[32m+[m[32mesac[m
[32m+[m
[32m+[m[32m# TXT[m
[32m+[m[32m[ -d $RESDIR ] || mkdir -p $RESDIR[m
[32m+[m[32mpushd $RESDIR[m
[32m+[m[32mfor II in W?? ; do[m
[32m+[m[32m    [ -d $II ] || continue[m
[32m+[m[32m    TARFILE=my$II.tar.bz2[m
[32m+[m[32m    TARFASC=$TARFILE.asc[m
[32m+[m[32m    rm -vf $TARFILE $TARFASC[m
[32m+[m[32m    echo "tar cfj $TARFILE $II/"[m
[32m+[m[32m    tar cfj $TARFILE $II/[m
[32m+[m[32m    echo "gpg --armor --output $TARFASC --encrypt --recipient $REC1 --recipient $REC2 $TARFILE"[m
[32m+[m[32m    gpg --armor --output $TARFASC --encrypt --recipient $REC1 --recipient $REC2 $TARFILE[m
[32m+[m[32mdone[m
[32m+[m[32mpopd[m
[32m+[m
[32m+[m[32mif [[ "$WEEK" != "W00" ]] && [[ "$WEEK" != "W01" ]] ; then[m
[32m+[m[32m    II="${RESDIR}my$WEEK.tar.bz2.asc"[m
[32m+[m[32m    echo "Check and move $II..."[m
[32m+[m[32m    [ -f $II ] && mv -vf $II .[m
[32m+[m[32mfi[m
[32m+[m
[32m+[m[32mecho "rm -f $SHA $SHA.asc"[m
[32m+[m[32mrm -f $SHA $SHA.asc[m
[32m+[m
[32m+[m[32mecho "sha256sum $FILES > $SHA"[m
[32m+[m[32msha256sum $FILES > $SHA[m
[32m+[m
[32m+[m[32mecho "# ################ CHECKSUM ###### #########"[m
[32m+[m[32mecho "sha256sum -c $SHA"[m
[32m+[m[32msha256sum -c $SHA[m
[32m+[m
[32m+[m[32mecho "# ################# SIGNING CHECKSUM ######### ######### ########"[m
[32m+[m[32mecho "gpg --output $SHA.asc --armor --sign --detach-sign $SHA"[m
[32m+[m[32mgpg --output $SHA.asc --armor --sign --detach-sign $SHA[m
[32m+[m
[32m+[m[32mecho "# ################# VERIFY ######### ######### ######### ########"[m
[32m+[m[32mecho "gpg --verify $SHA.asc $SHA"[m
[32m+[m[32mgpg --verify $SHA.asc $SHA[m
[32m+[m
[32m+[m[32mecho ""[m
[32m+[m[32mecho "==== ==== ==== ==== ==== ==== ==== ==== ==== ==== ==== ===="[m
[32m+[m[32mecho "==== ==== ==== ATTN: is this WEEK $WEEK ?? === ==== ==== ===="[m
[32m+[m[32mecho "==== ==== ==== ==== ==== ==== ==== ==== ==== ==== ==== ===="[m
[32m+[m[32mecho ""[m
[32m+[m
[32m+[m[32mexit 0[m
[32m+[m
